<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;org.demogrupsatu.facades.product.data.GenderData>"/>
	</bean>

	<bean class="org.demogrupsatu.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

	<bean class="org.demogrupsatu.facades.testimoni.TestimoniData">
		<property name="id" type="Integer" />
		<property name="name" type="String" />
		<property name="rating" type="Integer" />
		<property name="title" type="String" />
		<property name="description" type="String" />
		<property name="date" type="String" />
		<property name="halfStar" type="Boolean" />
	</bean>
</beans>
