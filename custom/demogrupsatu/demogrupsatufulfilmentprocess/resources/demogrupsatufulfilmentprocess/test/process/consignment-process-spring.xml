<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/> 
	<bean id="test.sendConsignmentToWarehouseAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.SendConsignmentToWarehouse"  parent="abstractAction"/>
	<bean id="test.waitBeforeTransmissionAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.WaitBeforeTransmission"  parent="abstractAction"/>
	<bean id="test.receiveConsignmentStatusAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.ReceiveConsignmentStatus"  parent="abstractAction"/>
	<bean id="test.confirmConsignmentPickupAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.ConfirmConsignmentPickup"  parent="abstractAction"/>
	<bean id="test.sendDeliveryMessageAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.SendDeliveryMessage"  parent="abstractAction"/>
	<bean id="test.sendReadyForPickupMessageAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.SendReadyForPickupMessage"  parent="abstractAction"/>
	<bean id="test.sendPickedUpMessageAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.SendPickedUpMessage"  parent="abstractAction"/>
	<bean id="test.cancelConsignmentAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.CancelConsignment"  parent="abstractAction"/>
	<bean id="test.sendCancelMessageAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.SendCancelMessage"  parent="abstractAction"/>
	<bean id="test.subprocessEndAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.SubprocessEnd"  parent="abstractAction"/>
	<bean id="test.allowShipmentAction" class="org.demogrupsatu.fulfilmentprocess.test.actions.consignmentfulfilment.AllowShipment"  parent="abstractAction">
		<property name="result" value="DELIVERY"></property>
	</bean>

</beans>
